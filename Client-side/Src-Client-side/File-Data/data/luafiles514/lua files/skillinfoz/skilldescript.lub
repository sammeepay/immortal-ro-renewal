[SKID.NC_MAINFRAME] = {"Remodel Mainframe", "MAX Lv : 4", "^777777Requirement :  Madogear License 4 ^000000", "ประเภท : ^777777  Passive ^000000", "รายละเอียด : ^777777   ปรับปรุง Madogear ให้มีพลังป้องกันสุงขึ้นและเพิ่มค่า Over Heat Limit ของ Magic Gear เมื่อ Magic Gear อยู่ในสถานะ Over Heat แล้วจะทำให้ Magic Gear ได้รับความเสียหายอย่างต่อ เนื่อง ซึ่งค่า Over Heat Limit ที่สูงขึ้นจะเพิ่มจุดที่ทำให้เกิดสถานะ Over Heat ให้สูงขึ้น ^000000", "[Level 1] : ^777777 พลังป้องกัน +40 / Over Heat Limit + 200 ^000000", "[Level 2] : ^777777 พลังป้องกัน +60 / Over Heat Limit + 280 ^000000", "[Level 3] : ^777777 พลังป้องกัน +80 / Over Heat Limit + 360 ^000000", "[Level 4] : ^777777 พลังป้องกัน +100 / Over Heat Limit + 450 ^000000"}, 
[SKID.NC_SELFDESTRUCTION] = {"Suicidal Destruction", "MAX Lv : 3", "^777777Requirement :  Remodel Mainframe 2 ^000000", "ประเภท : ^777777  Active / Damage  ^000000", "รายละเอียด : ^777777  ทำการระเบิดตัวเอง สร้างความเสียหายอย่างรุนแรงในวงกว้าง เมื่อใช้สกิลนี้จะสูญเสีย Madogear ไป และจะใช้ SP ที่มีอยู่ทั้งหมด ไม่สามารถขัดขวางการร่ายสกิลนี้ได้ ต้องการไอเทม Fuel Canister 3 ea และต้องมี ไอเทม <Suicidal Device> ในการใช้สกิล^000000", "[Level 1] : ^777777 ระยะสกิล 5 x 5 ช่อง ^000000", "[Level 2] : ^777777 ระยะสกิล 7 x 7 ช่อง ^000000", "[Level 3] : ^777777 ระยะสกิล 9 x 9 ช่อง ^000000"}, 
[SKID.NC_SHAPESHIFT] = {"Shift Shape", "MAX Lv : 4", "^777777Requirement :  Remodel Mainframe 2  ^000000", "ประเภท : ^777777  Active / Buff ^000000", "รายละเอียด : ^777777 ทำการเปลี่ยนธาตุของ Madogear ต้องมีไอเทม Shape Shift ในการใช้สกิล และต้องการไอเทม Fuel Canister 2 ea / 1 Enchanted Stone และต้องมี ไอเทม <Shape Shifter> ในการใช้สกิล ^000000", "[Level 1] : ^777777 เปลี่ยนเป็นธาตุ fire property / ต้องการไอเทม 3 Scarlet Point ในการใช้สกิล^000000", "[Level 2] : ^777777 เปลี่ยนเป็นธาตุ earth property / ต้องการไอเทม 3 Lime Green Point ในการใช้สกิล^000000", "[Level 3] : ^777777 เปลี่ยนเป็นธาตุ wind property / ต้องการไอเทม 3 Yellow Wish Point ในการใช้สกิล^000000", "[Level 4] : ^777777 เปลี่ยนเป็นธาตุ water property / ต้องการไอเทม 3 Indigo Point ในการใช้สกิล^000000"}, 
[SKID.NC_EMERGENCYCOOL] = {"Cool Emergency", "MAX Lv : 1", "^777777Requirement :  Suicidal Destruction 2  ^000000", "ประเภท : ^777777  Active / Buff ^000000", "รายละเอียด : ^777777 ทำให้ Madogear ที่อยู่ในสถานะ Over Heat เย็นลงได้ ต้องมีไอเทม Cooling System ในการใช้สกิล และต้องการไอเทม Fuel Canister 1 ea และต้องมี ไอเทม <Cooling Device> ในการใช้สกิล^000000"}, 
[SKID.NC_INFRAREDSCAN] = {"Infra Red Scan", "MAX Lv : 1", "^777777Requirement :  Shift Shape  2  ^000000", "ประเภท : ^777777  Active / Detect / Debuff ^000000", "รายละเอียด : ^777777 ตรวจจับเป้าหมายที่ซ่อนตัวอยู่ในระยะ 15 x 15 ช่อง และมีโอกาส 30 % ที่จะลดอัตราการหลบหลีกของเป้าหมายลง เมื่อเข้ามาอยู่ในระยะของสกิล ^000000"}, 
