[SKID.WL_CRIMSONROCK] = {"Crimson Rock", "MAX LV 5", "^777777Requirement : Summon Fireball 1  ^000000", "ประเภท : ^777777 Active / Damage  ^000000", "รายละเอียด : ^777777 เรียกอุกกาบาตขนาดยักษ์ลงมาจากท้องฟ้า เป้าหมายที่อยู่ในระยะจะได้รับความเสียหายธาตุไฟ แรงกระแทกจากอุกกาบาตจะส่งผลให้เกิดคลื่นความเสียหาย ทำให้เกิดความเสียหายได้เล็กน้อย มีโอกาสทำให้เป้าหมายติดสถานะ Stun  ^000000", "[Level 1] : ^777777 สร้างความเสียหายธาตุไฟ 1600%  ^000000", "[Level 2] : ^777777 สร้างความเสียหายธาตุไฟ 1900%  ^000000", "[Level 3] : ^777777 สร้างความเสียหายธาตุไฟ 2200%  ^000000", "[Level 4] : ^777777 สร้างความเสียหายธาตุไฟ 2500%  ^000000", "[Level 5] : ^777777 สร้างความเสียหายธาตุไฟ 2800%  ^000000"}, 
[SKID.WL_HELLINFERNO] = {"Hell Inferno", "MAX LV 5", "^777777Requirement : Crimson Rock 2  ^000000", "ประเภท : ^777777  Damage / Curse  ^000000", "รายละเอียด : ^777777 เรียกไฟแห่งนรกออกมาโจมตีเป้าหมาย สร้างความเสียหายด้วยธาตุไฟ และ มืด, ทำให้เป้าหมายติดสถานะ Ignite จะทำให้ไฟลุกไหม้อยู่บนร่างกาย ซึ่งทำให้เกิดความเสียหายธาตุไฟอย่างต่อเนื่อง และลด MDEF ลง ^000000", "[Level 1] : ^777777 Fire Property Damage 60% / Dark Property Magic Damage 240% ^000000", "[Level 2] : ^777777 Fire Property Damage 120% / Dark Property Magic Damage 480% ^000000", "[Level 3] : ^777777 Fire Property Damage 180% / Dark Property Magic Damage 720% ^000000", "[Level 4] : ^777777 Fire Property Damage 240% / Dark Property Magic Damage 960% ^000000", "[Level 5] : ^777777 Fire Property Damage 300% / Dark Property Magic Damage 1200%  ^000000"}, 
[SKID.WL_COMET] = {"Comet", "MAX LV 5", "^777777Requirement : Hell Inferno 3  ^000000", "ประเภท : ^777777  Damage / Curse  ^000000", "รายละเอียด : ^777777 โจมตีเป้าหมายด้วยดาวหางจากอวกาศ ทำให้เกิดระเบิดครั้งใหญ่สร้างความเสียหาย 19x19 ช่อง  มีโอกาสทำให้เป้าหมายติดสถานะ Ignite ตลอดระยะเวลาของสกิล เป้าหมายที่อยู่บริเวณตรงกลาง จะได้รับความเสียหายมากที่สุด (ใช้ 2 Red Gemstone)  หากมี Warlock อยู่ใน party หรืออยู่ใกล้ๆ จะสามารถร่ายได้โดยไม่จำเป็นต้องใช้ Gem stone และจะไม่ลดค่าความเสียหาย ถึงแม้เป้าหมายจะไม่อยู่บริเวณตรงกลางของสกิล ^000000", "[Level 1] : ^777777 Neutral property Magic Damage 1500% ~ 3000% ^000000", "[Level 2] : ^777777 Neutral property Magic Damage 2000% ~ 3500% ^000000", "[Level 3] : ^777777 Neutral property Magic Damage 2500% ~ 4000% ^000000", "[Level 4] : ^777777 Neutral property Magic Damage 3000% ~ 4500% ^000000", "[Level 5] : ^777777 Neutral property Magic Damage 3500% ~ 5000% ^000000"}, 
