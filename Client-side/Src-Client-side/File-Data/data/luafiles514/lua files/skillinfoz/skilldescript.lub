[SKID.NC_AXEBOOMERANG] = {"Axe Boomerang ", "MAX Lv : 5", "^777777Requirement :  Axe Mastery 1  ^000000", "ประเภท : ^777777  Active / Damage ^000000", "รายละเอียด : ^777777  ขว้างขวานออกไปเพื่อทำความเสียหายในระยะไกล ทำให้เป้าหมายกระเด็นออกไป, น้ำหนักของขวานมีผลต่อความเสียหายของสกิลนี้ ของสกิลสามารถเพิ่มขึ้นได้จาก Base Level ของผู้ใช้ ^000000", "[Level 1] : ^777777  พลังโจมตี 300% + น้ำหนักของขวาน / ระยะของสกิล 5 ช่อง  ^000000", "[Level 2] : ^777777  พลังโจมตี 350% + น้ำหนักของขวาน / ระยะของสกิล 6 ช่อง  ^000000", "[Level 3] : ^777777  พลังโจมตี 400% + น้ำหนักของขวาน / ระยะของสกิล 7 ช่อง  ^000000", "[Level 4] : ^777777  พลังโจมตี 450% + น้ำหนักของขวาน / ระยะของสกิล 8 ช่อง ^000000", "[Level 5] : ^777777  พลังโจมตี 500% + น้ำหนักของขวาน / ระยะของสกิล 9 ช่อง  ^000000"}, 
[SKID.NC_POWERSWING] = {"Power Swing", "MAX Lv : 5", "^777777Requirement :  Axe Boomerang 3  ^000000", "ประเภท : ^777777  Active / Damage ^000000", "รายละเอียด : ^777777  เหวี่ยงขวานไปยังศัตรู 1 เป้าหมาย มีโอกาสทำให้ติดสถานะ Stun, มีโอกาสที่จะใช้สกิล Axe Boomerang อัตโนมัติหลังจากใช้งานสกิลนี้ ความรุนแรงของสกิลสามารถเพิ่มขึ้นได้จาก STR , DEX และ Base Level ของผู้ใช้ ^000000", "[Level 1] : ^777777  พลังโจมตี 400%  ^000000", "[Level 2] : ^777777  พลังโจมตี 500%  ^000000", "[Level 3] : ^777777  พลังโจมตี 600%  ^000000", "[Level 4] : ^777777  พลังโจมตี 700%  ^000000", "[Level 5] : ^777777  พลังโจมตี 800%  ^000000"}, 
[SKID.NC_AXETORNADO] = {"Axe Tornado ", "MAX Lv : 5", "^777777Requirement :  Axe Mastery 1  ^000000", "ประเภท : ^777777  Active / Damage ^000000", "รายละเอียด : ^777777  หมุนขวานไปรอบตัวด้วยความเร็วสูง สร้างความเสียหายแก่เป้าหมายโดยรอบ ค่า VIT ที่สูงขึ้นจะส่งผลให้ความเสียหายเพิ่มขึ้น ความรุนแรงของสกิลสามารถเพิ่มขึ้นได้หากสวมใส่อาวุธธาตุลม , รวมทั้งค่า VIT และ Base Level ของผู้ใช้ แต่จะใช้ HP ส่วนนึงในการร่ายสกิล^000000", "[Level 1] : ^777777  พลังโจมตี 300%, ระยะสกิล 5 x 5  ช่อง / ใช้ 20 HP ในการร่ายสกิล ^000000", "[Level 2] : ^777777  พลังโจมตี 400%, ระยะสกิล 5 x 5  ช่อง / ใช้ 40 HP ในการร่ายสกิล ^000000", "[Level 3] : ^777777  พลังโจมตี 500%, ระยะสกิล 7 x 7  ช่อง / ใช้ 60 HP ในการร่ายสกิล ^000000", "[Level 4] : ^777777  พลังโจมตี 600%, ระยะสกิล 7 x 7  ช่อง / ใช้ 80 HP ในการร่ายสกิล ^000000", "[Level 5] : ^777777  พลังโจมตี 700%, ระยะสกิล 7 x 7  ช่อง / ใช้ 100 HP ในการร่ายสกิล ^000000"}, 
