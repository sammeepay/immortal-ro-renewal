[SKID.WM_LESSON] = {"Lesson", "MAX Lv : 10", "^777777Requirement : Wanderer, Minstrel Basic Skill^000000", "ประเภท : ^777777  Passive ^000000", "รายละเอียด : ^777777 เพิ่ม Max SP และ SP Recovery ที่เลเวล 5 ~ 10 สามารถใช้ทักษะร้องเพลง,เต้นของ 3rd Class ในขณะที่กำลังใช้สกิลร้องเพลง,เต้นและสกิลการแสดงคู่ของ 2nd Class ได้  ^000000", "[Level 1] : ^777777 MSP +30  ^000000", "[Level 2] : ^777777 MSP +60   ^000000", "[Level 3] : ^777777 MSP +90  ^000000", "[Level 4] : ^777777 MSP +120  ^000000", "[Level 5] : ^777777 MSP +150  / สามารถใช้สกิลร้องเพลงของ 3rd Class ขณะใช้สกิลร้องเพลงของ 2nd Class ได้ ^000000", "[Level 6] : ^777777 MSP +180  / สามารถใช้สกิลร้องเพลงของ 3rd Class ขณะใช้สกิลร้องเพลงของ 2nd Class ได้^000000", "[Level 7] : ^777777 MSP +200  / สามารถใช้สกิลร้องเพลงของ 3rd Class ขณะใช้สกิลร้องเพลงของ 2nd Class ได้^000000", "[Level 8] : ^777777 MSP +240  / สามารถใช้สกิลร้องเพลงของ 3rd Class ขณะใช้สกิลร้องเพลงของ 2nd Class ได้^000000", "[Level 9] : ^777777 MSP +270  / สามารถใช้สกิลร้องเพลงของ 3rd Class ขณะใช้สกิลร้องเพลงของ 2nd Class ได้^000000", "[Level 10] : ^777777 MSP +300 / สามารถใช้สกิลร้องเพลงของ 3rd Class ขณะใช้สกิลร้องเพลงของ 2nd Class ได้^000000"}, 
[SKID.WM_METALICSOUND] = {"Metalic Sound", "MAX Lv : 5", "^777777Requirement : Dominion Impulse 1 ^000000", "ประเภท : ^777777  Active / Damage ^000000", "รายละเอียด : ^777777 ปล่อยเสียงระดับสูงใส่ 1 เป้าหมาย สร้างความเสียหายแก่ HP และ SP, ความเสียหายจะมากยิ่งขึ้นหากเป้าหมายกำลังหลับอยู่ , สกิล Lesson สามารถช่วยเพิ่มความรุนแรงของสกิลนี้ได้ ^000000", "[Level 1] : ^777777 สร้างความเสียหายทางเวทย์มนตร์ 120% + สกิล Lesson ที่เรียนรู้ ^000000", "[Level 2] : ^777777 สร้างความเสียหายทางเวทย์มนตร์ 240% + สกิล Lesson ที่เรียนรู้  ^000000", "[Level 3] : ^777777 สร้างความเสียหายทางเวทย์มนตร์ 360% + สกิล Lesson ที่เรียนรู้  ^000000", "[Level 4] : ^777777 สร้างความเสียหายทางเวทย์มนตร์ 480% + สกิล Lesson ที่เรียนรู้  ^000000", "[Level 5] : ^777777 สร้างความเสียหายทางเวทย์มนตร์ 600% + สกิล Lesson ที่เรียนรู้  ^000000"}, 
[SKID.WM_REVERBERATION] = {"Reverberation", "MAX Lv : 5", "^777777Requirement : Unchained Serenade หรือ Hip Shaker 5 ^000000", "ประเภท : ^777777  Active / Damage ^000000", "รายละเอียด : ^777777 สร้าง คลื่นเสียงระดับสูงจากพื้นดิน หากเป้าหมายออกจากพื้นที่สกิล หรือก่อนผลของสกิลจะหมดลงเป้าหมายที่อยู่ในขอบเขตของสกิลระยะ 3 x 3 ช่องจะได้รับความเสียหาย การดังสะท้อนสามารถยกเลิกได้หากได้รับการโจมตีทางกายภาพระยะไกลหรือเวทย์มนต์^000000", "[Level 1] : ^777777  พลังโจมตี 400% + MATK 200% ^000000", "[Level 2] : ^777777  พลังโจมตี 500% + MATK 300%  ^000000", "[Level 3] : ^777777  พลังโจมตี 600% + MATK 400%   ^000000", "[Level 4] : ^777777  พลังโจมตี 700% + MATK 500%   ^000000", "[Level 5] : ^777777  พลังโจมตี 800% + MATK 600%   ^000000"}, 
[SKID.WM_DOMINION_IMPULSE] = {"Dominion Impulse", "MAX Lv : 1", "^777777Requirement : Reverberation 1 ^000000", "ประเภท : ^777777  Active / Special ^000000", "รายละเอียด : ^777777 เพิ่มระยะการสะท้อนของเสียงเป็น 11 x 11 ช่องรอบตัว สามารถใช้ทับซ้อนกับ การสะท้อนของเสียงจากผู้อื่นได้ ^000000"}, 
[SKID.WM_SEVERE_RAINSTORM] = {"Severe Rainstorm", "MAX Lv : 5", "^777777Requirement : Slinging Arrow หรือ Melody Strike 5 ^000000", "ประเภท : ^777777  Active / Damage ^000000", "รายละเอียด : ^777777 ปล่อยลูกธนูจำนวนนับไม่ถ้วนขึ้นฟ้า และตกลงมาในระยะ 11 x 11 ช่อง ความแรงของสกิลขึ้นอยู่กับค่า AGI และ DEX ต้องการลูกธนู 20 ea หากมีลูกธนูไม่ถึง 20 ea การใช้สกิลจะล้มเหลว ^000000", "[Level 1] : ^777777 1 hit ทุกๆ  0.3 วินาที / ระยะเวลา 0.9 วินาที ^000000", "[Level 2] : ^777777 1 hit ทุกๆ  0.3 วินาที / ระยะเวลา 1.5 วินาที  ^000000", "[Level 3] : ^777777 1 hit ทุกๆ  0.3 วินาที / ระยะเวลา 2.1 วินาที   ^000000", "[Level 4] : ^777777 1 hit ทุกๆ  0.3 วินาที / ระยะเวลา 2.7 วินาที  ^000000", "[Level 5] : ^777777 1 hit ทุกๆ  0.3 วินาที / ระยะเวลา 3.0 วินาที  ^000000"}, 
[SKID.WM_POEMOFNETHERWORLD] = {"Despair Song", "MAX Lv : 5", "^777777Requirement : Lesson 1 ^000000", "ประเภท : ^777777  Active / Debuff ^000000", "รายละเอียด : ^777777  สร้างคลื่นเสียงชนิดพิเศษ ลงพื้นดิน ทำให้เป้าหมายในระยะไม่สามารถเคลื่อนที่ได้ สามารถร่ายสกิลสูงสุดได้ 5 ครั้งในเวลาเดียวกัน ไม่สามารถใช้กับ Boss Monster  เลเวลของผู้ใช้และเป้าหมายมีผลต่อระยะเวลาที่เคลื่อนที่ไม่ได้ [ขอบเขตของสกิล 3 x 3 ช่อง] ต้องการ 1 [Protect Neck Candy]. ^000000", "[Level 1] : ^777777 ระยะเวลาของสกิล 8 วินาที / Sound Wave Duration 9 วินาที ^000000", "[Level 2] : ^777777 ระยะเวลาของสกิล 10 วินาที / Sound Wave Duration 11 วินาที ^000000", "[Level 3] : ^777777 ระยะเวลาของสกิล 12 วินาที / Sound Wave Duration 13 วินาที ^000000", "[Level 4] : ^777777 ระยะเวลาของสกิล 14 วินาที / Sound Wave Duration 15 วินาที ^000000", "[Level 5] : ^777777 ระยะเวลาของสกิล 16 วินาที / Sound Wave Duration 17 วินาที ^000000"}, 
[SKID.WM_VOICEOFSIREN] = {"Voice Of Siren", "MAX Lv : 5", "^777777Requirement : Despair Song 3 ^000000", "ประเภท : ^777777  Active / Debuff ^000000", "รายละเอียด : ^777777 สร้างความสนใจแก่เป้าหมายในระยะของสกิล และทำให้เป้าหมายคล้อยตาม  มอนสเตอร์จะไม่โจมตีเรา และระหว่างผลของสกิลเราจะไม่สามารถโจมตีเป้าหมายได้เช่นกัน เลเวลของเป้าหมายมีผลต่อระยะเวลาที่มีผล และผลของสกิลจะถูกยกเลิกเมื่อเป้าหมายได้รับการโจมตี ^000000", "[Level 1] : ^777777 ระยะโจมตี 5 x 5  / ระยะเวลา 15 วินาที ^000000", "[Level 2] : ^777777 ระยะโจมตี 7 x 7  / ระยะเวลา 18 วินาที ^000000", "[Level 3] : ^777777 ระยะโจมตี 9 x 9  / ระยะเวลา 21 วินาที  ^000000", "[Level 4] : ^777777 ระยะโจมตี 11 x 11  / ระยะเวลา 24 วินาที  ^000000", "[Level 5] : ^777777 ระยะโจมตี 13 x 13  / ระยะเวลา 27 วินาที  ^000000"}, 
[SKID.WM_DEADHILLHERE] = {"At Dead Hill", "MAX Lv : 5", "^777777Requirement : Circling Nature 3 ^000000", "ประเภท : ^777777  Active / Recovery ^000000", "รายละเอียด : ^777777 ฟื้นคืนชีวิตให้กับ 1 เป้าหมาย โดยเป้าหมายจะฟื้นขึ้นมาโดยฟื้น HP เท่ากับ SP ที่เหลือตอนตาย ถ้าเป้าหมายมี SP 0 จะฟื้นขึ้นมาโดยมี HP 1  ต้องการ 1 [Heartbroken Tears] ^000000", "[Level 1] : ^777777 โอกาศสำเร็จ 90% / ใช้ 50% ของ SP ตอนเป้าหมายมีชีวิต ^000000", "[Level 2] : ^777777 โอกาศสำเร็จ 92% / ใช้ 40% ของ SP ตอนเป้าหมายมีชีวิต  ^000000", "[Level 3] : ^777777 โอกาศสำเร็จ 94% / ใช้ 30% ของ SP ตอนเป้าหมายมีชีวิต  ^000000", "[Level 4] : ^777777 โอกาศสำเร็จ 96% / ใช้ 20% ของ SP ตอนเป้าหมายมีชีวิต  ^000000", "[Level 5] : ^777777 โอกาศสำเร็จ 98% / ใช้ 10% ของ SP ตอนเป้าหมายมีชีวิต  ^000000"}, 
[SKID.WM_LULLABY_DEEPSLEEP] = {"Lullaby", "MAX Lv : 5", "^777777Requirement : Lesson 1 ^000000", "ประเภท : ^777777  Active / Debuff ^000000", "รายละเอียด : ^777777 ทำให้เป้าหมายที่อยู่รอบๆตัวเราตกอยู่ในสภานะ Deep Sleep' ในขณะที่ตกอยู่ในสถานะ 'Deep Sleep' จะไม่สามารถเคลื่อนที่ โจมตี ใช้ไอเทมหรือสกิลได้โดยหากเป้าหมายได้รับการโจมตีจะเป็นการยกเลิกสถานะ 'Deep Sleep' แต่ในขณะที่ตกอยู่ในสถานะ 'Deep Sleep' จะได้รับความเสียหายเป็น 1.5 เท่า และผู้ตกอยู่ในสถานะ 'Deep Sleep' จะฟื้นฟู HP/SP 3% ทุกๆ 2 วินาที^000000", "[Level 1] : ^777777 ระยะ 11 x 11 ^000000", "[Level 2] : ^777777 ระยะ 13 x 13 ^000000", "[Level 3] : ^777777 ระยะ 15 x 15 ^000000", "[Level 4] : ^777777 ระยะ 17 x 17 ^000000", "[Level 5] : ^777777 ระยะ 19 x 19 ^000000"}, 
[SKID.WM_SIRCLEOFNATURE] = {"Circling Nature", "MAX Lv : 5", "^777777Requirement : Lesson 1 ^000000", "ประเภท : ^777777  Active / Recovery ^000000", "รายละเอียด : ^777777 สกิลนี้มีผลต่อผู้เล่นที่อยู่ในระยะทุกคน , ฟื้นฟู HP และลด SP หากผู้เล่นไม่มี SP เหลืออยู่จะไม่ได้รับผลของสกิลนี้^000000", "[Level 1] : ^777777 ระยะโจมตี 7 x 7  / SP ลดลง 4 ทุกๆ วินาที/ฟื้นฟู 40 HPต่อ วินาที^000000", "[Level 2] : ^777777 ระยะโจมตี 9 x 9 / SP ลดลง 8 ทุกๆ  วินาที/ ฟื้นฟู 80 HPต่อ วินาที^000000", "[Level 3] : ^777777 ระยะโจมตี 11 x 11 / SP ลดลง 12 ทุกๆ  วินาที/ ฟื้นฟู 120 HPต่อ วินาที^000000", "[Level 4] : ^777777 ระยะโจมตี 13 x 13 / SP ลดลง 16 ทุกๆ  วินาที/ ฟื้นฟู 160 HPต่อ วินาที^000000", "[Level 5] : ^777777 ระยะโจมตี 15 x 15 / SP ลดลง 20 ทุกๆ  วินาที/ ฟื้นฟู 200 HPต่อ วินาที^000000"}, 
