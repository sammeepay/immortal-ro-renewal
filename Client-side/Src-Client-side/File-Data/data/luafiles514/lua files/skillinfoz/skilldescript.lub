[SKID.GN_REMODELING_CART] = {"Cart Remodeling", "MAX Lv : 5", "^777777Requirement : Genetic Basic^000000", "ประเภท : ^777777  Passive ^000000", "รายละเอียด : ^777777 ปรับแต่งรถเข็นของผู้ใช้ , เพิ่มน้ำหนักสูงสุด , เพิ่มความแม่นยำเมื่อใช้สกิล Cart Revolution , Cart Tornado และ Cart Cannon ^000000", "[Level 1] : ^777777 Weight + 500 / Cart skill HIT + 4  ^000000", "[Level 2] : ^777777 Weight + 1000 / Cart skill HIT + 8  ^000000", "[Level 3] : ^777777 Weight + 1500 / Cart skill HIT + 12  ^000000", "[Level 4] : ^777777 Weight + 2000 / Cart skill HIT + 16  ^000000", "[Level 5] : ^777777 Weight + 2500 / Cart skill HIT + 20  ^000000"}, 
[SKID.GN_CART_TORNADO] = {"Cart Tornado Attack", "MAX Lv : 5", "^777777Requirement : Cart Remodeling 1^000000", "ประเภท : ^777777  Active / Damage ^000000", "รายละเอียด : ^777777 เหวี่ยงรถเข็นรอบๆตัวและกระแทกเป้าหมายในระยะ ให้กระเด็นถอยหลัง 2 ช่องและ ศัตรูในระยะ 2 ช่อง , ต้องการรถเข็นในการใช้สกิลนี้ สกิล Cart Remodeling และน้ำหนักในรถเข็น สามารถเพิ่มความรุนแรงของสกิลนี้ได้^000000", "[Level 1] : ^777777  พลังโจมตี 50% ^000000", "[Level 2] : ^777777  พลังโจมตี 100% ^000000", "[Level 3] : ^777777  พลังโจมตี 150% ^000000", "[Level 4] : ^777777  พลังโจมตี 200% ^000000", "[Level 5] : ^777777  พลังโจมตี 250% ^000000"}, 
[SKID.GN_CARTCANNON] = {"Cart Cannon", "MAX Lv : 5", "^777777Requirement : Cart Remodeling 2^000000", "ประเภท : ^777777  Active / Damage ^000000", "รายละเอียด : ^777777 ยิงกระสุนปืนจากรถเข็นสร้างความเสียหายอย่างมากแก่ 1 เป้าหมาย , ต้องการรถเข็นในการใช้สกิลนี้ สกิล Cart Remodeling และค่า INT ของผู้ใช้สามารถเพิ่มความรุนแรงของสกิลนี้ได้อย่างมาก ต้องการ 1 Cannon Ball ^000000", "[Level 1] : ^777777  พลังโจมตี 60% +  Remodeling level ^000000", "[Level 2] : ^777777  พลังโจมตี 120% +  Remodeling level ^000000", "[Level 3] : ^777777  พลังโจมตี 180% +  Remodeling level ^000000", "[Level 4] : ^777777  พลังโจมตี 240% +  Remodeling level ^000000", "[Level 5] : ^777777  พลังโจมตี 300% +  Remodeling level ^000000"}, 
[SKID.GN_CARTBOOST] = {"Cart Boost", "MAX Lv : 5", "^777777Requirement : Cart Remodeling 3^000000", "ประเภท : ^777777  Buff (To yourself)  ^000000", "รายละเอียด : ^777777 เพิ่มความเร็วในการเคลื่อนที่ และ Attack Power เป็นเวลา 90 วินาที , ต้องการรถเข็นในการใช้สกิลนี้^000000", "[Level 1] : ^777777 ความเร็วในการเคลื่อนที่เพิ่มขึ้น 50% / พลังโจมตีเพิ่มขึ้น 10 ^000000", "[Level 2] : ^777777 ความเร็วในการเคลื่อนที่เพิ่มขึ้น 50% / พลังโจมตีเพิ่มขึ้น 20 ^000000", "[Level 3] : ^777777 ความเร็วในการเคลื่อนที่เพิ่มขึ้น 75% / พลังโจมตีเพิ่มขึ้น 30 ^000000", "[Level 4] : ^777777 ความเร็วในการเคลื่อนที่เพิ่มขึ้น 75% / พลังโจมตีเพิ่มขึ้น 40 ^000000", "[Level 5] : ^777777 ความเร็วในการเคลื่อนที่เพิ่มขึ้น 100% / พลังโจมตีเพิ่มขึ้น 50 ^000000"}, 
[SKID.GN_THORNS_TRAP] = {"Thorns Trap", "MAX Lv : 5", "^777777Requirement : Special Pharmacy 2 ^000000", "ประเภท : ^777777  Special / Damage ^000000", "รายละเอียด : ^777777 โยนกัปดักพืชหนามแหลมลงบนพื้นทำให้เป้าหมายเคลื่อนที่ไม่สะดวก , หากเป้าหมายยืนอยู่บน Trap จะได้รับความเสียหายตลอดเวลา และหากเป้าหมายได้รับความเสียหายจากการโจมตี ธาตุไฟ กับดักจะถูกทำลาย ต้องการ 1 Seed Of Horny Plant , Thorns Tap สามารถใช้ได้พร้อมกันสูงสุด 3 ครั้ง ^000000", "[Level 1] : ^777777 ระยะเวลา 10 วินาที  ^000000", "[Level 2] : ^777777 ระยะเวลา 12 วินาที  ^000000", "[Level 3] : ^777777 ระยะเวลา 14 วินาที  ^000000", "[Level 4] : ^777777 ระยะเวลา 16 วินาที  ^000000", "[Level 5] : ^777777 ระยะเวลา 18 วินาที  ^000000"}, 
[SKID.GN_BLOOD_SUCKER] = {"Blood Sucker", "MAX Lv : 5", "^777777Requirement : Special Pharmacy 3 ^000000", "ประเภท : ^777777  Special / Damage ^000000", "รายละเอียด : ^777777 หว่านเมล็ดพืชสูบเลือดลงบนเป้าหมาย , เมื่อเป้าหมายได้รับความเสียหาย ผู้ใช้จะได้รับHP บางส่วน , สามารถใช้งานพร้อมกันได้ 3 เป้าหมาย หากผู้ใช้และเป้าหมายอยู่ห่างกันเกิน 12 ช่องผลของสกิลจะถูกยกเลิก ต้องการ 1 Bloodsuck Plant Seed ^000000", "[Level 1] : ^777777 ระยะเวลา 20 วินาที  ^000000", "[Level 2] : ^777777 ระยะเวลา 22 วินาที  ^000000", "[Level 3] : ^777777 ระยะเวลา 24 วินาที  ^000000", "[Level 4] : ^777777 ระยะเวลา 26 วินาที  ^000000", "[Level 5] : ^777777 ระยะเวลา 28 วินาที  ^000000"}, 
[SKID.GN_SPORE_EXPLOSION] = {"Spore Explosion", "MAX Lv : 5", "^777777Requirement : Special Pharmacy 4 ^000000", "ประเภท : ^777777  Special / Damage ^000000", "รายละเอียด : ^777777โปรยผงสปอร์เห็ดระเบิดลงบนเป้าหมาย , หลังจากผ่านไปครู่หนึ่ง สปอร์เห็ดจะระเบิด โดยจะไม่ได้สร้างความเสียหายแก่เป้าหมาย แต่จะสร้างความเสียหายทางเวทย์มนต์ให้แก่ทุกๆคนที่อยู่รอบๆเป้าหมาย , สกิลเลเวลจะช่วยเพิ่มระยะของสกิลนี้ได้ ต้องการ 1 Bomb Mushroom Spore ^000000", "[Level 1] : ^777777 ระยะ 3 x 3 ช่อง / ระเบิดใน 2 วินาที^000000", "[Level 2] : ^777777 ระยะ 5 x 5 ช่อง / ระเบิดใน 2 วินาที^000000", "[Level 3] : ^777777 ระยะ 7 x 7 ช่อง / ระเบิดใน 3 วินาที^000000", "[Level 4] : ^777777 ระยะ 9 x 9 ช่อง / ระเบิดใน 3 วินาที^000000", "[Level 5] : ^777777 ระยะ 11 x 11 ช่อง / ระเบิดใน 4 วินาที^000000"}, 
