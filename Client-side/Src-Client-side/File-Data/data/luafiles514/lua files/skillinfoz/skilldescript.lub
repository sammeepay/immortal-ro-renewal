[SKID.NC_FLAMELAUNCHER] = {"Flame Launcher", "MAX Lv : 3", "^777777Requirement :  Vulcan Arm 1 ^000000", "ประเภท : ^777777  Active / Damage ^000000", "รายละเอียด : ^777777  พ่นไฟลงบนพื้น สร้างความเสียหายธาตุไฟแก่ศัตรูที่อยู่ในบริเวณ และมีโอกาสทำให้ติดสถานะ [Ignition] ต้องการไอเทม 1 Fuel Canister และต้องสวมใส่ หรือมีไอเทม Flame Thrower ในการใช้สกิล การร่ายสกิลไม่สามารถถูกขัดขวางได้ , Damage ของสกิลสามารถเพิ่มขึ้นได้จาก Base Level ของผู้ใช้ ^000000", "[Level 1] : ^777777 สร้างความเสียหายธาตุไฟ 600 % / โอกาสในการติดสถานะ [Ignition] 30%^000000", "[Level 2] : ^777777 สร้างความเสียหายธาตุไฟ 900 % / โอกาสในการติดสถานะ [Ignition] 40%^000000", "[Level 3] : ^777777 สร้างความเสียหายธาตุไฟ 1200 % / โอกาสในการติดสถานะ [Ignition] 50%^000000"}, 
[SKID.NC_COLDSLOWER] = {"Cold Slower", "MAX Lv : 3", "^777777Requirement :  Vulcan Arm 3 ^000000", "ประเภท : ^777777  Active / Damage ^000000", "รายละเอียด : ^777777  ใช้ Magic Gear ในการสร้างความเย็น สร้างความเสียหายธาตุน้ำแก่เป้าหมายในบริเวณ และทำให้เป้าหมายติดสถานะ [Freezing] ต้องการไอเทม 1 Fuel Canister และไอเทม Liquid Cold Bullet ในการใช้สกิล การร่ายสกิลไม่สามารถถูกขัดขวางได้ , Damage ของสกิลสามารถเพิ่มขึ้นได้จาก Base Level ของผู้ใช้ ^000000", "[Level 1] : ^777777 สร้างความเสียหายธาตุน้ำ 600 % / ระยะ 5 x 5 ช่อง ^000000", "[Level 2] : ^777777 สร้างความเสียหายธาตุน้ำ 900 % / ระยะ 7 x 7 ช่อง ^000000", "[Level 3] : ^777777 สร้างความเสียหายธาตุน้ำ 1200 % / ระยะ 9 x 9 ช่อง ^000000"}, 
[SKID.NC_ARMSCANNON] = {"Arm Cannon", "MAX Lv : 3", "^777777Requirement : Flame Launcher 2 / Cold Slower 2  ^000000", "ประเภท : ^777777  Active / Damage ^000000", "รายละเอียด : ^777777  ปล่อยแขนของ Madogear ออกไป สร้างความเสียหายแก่เป้าหมายในบริเวณนั้น ความเสียหายจะลงเมื่อกลุ่มของเป้าหมายมีจำนวนมากขึ้น การร่ายสกิลไม่สามารถถูกขัดขวางได้ , Damage property เปลี่ยนแปลงตาม ชนิดของ cannon ball, Damage ของสกิลสามารถเพิ่มขึ้นได้จาก Base Level ของผู้ใช้ และแรงขึ้นหากโจมตีศัตรูขนาดเล็ก ต้องการ Fuel Canister และสวมใส่ Cannon Ball (ชนิดใดก็ได้ 1 ชนิด) อย่างละ 1 ea ในการใช้สกิล ^000000", "[Level 1] : ^777777  พลังโจมตี 700% / 650% / 600%   / ระยะสกิล 9 ช่อง /   ระยะ 7 x 7 ช่อง ^000000", "[Level 2] : ^777777  พลังโจมตี 1100% / 1000% / 900%   / ระยะสกิล 11 ช่อง /   ระยะ 5 x 5 ช่อง ^000000", "[Level 3] : ^777777  พลังโจมตี 1500% / 1350% / 1200%   / ระยะสกิล 13 ช่อง /  ระยะ 3 x 3 ช่อง ^000000"}, 
[SKID.NC_ACCELERATION] = {"Acceleration", "MAX Lv : 3", "^777777Requirement :  Madogear License 1 ^000000", "ประเภท : ^777777  Active / Buff ^000000", "รายละเอียด : ^777777  เพิ่มความเร็วในการเคลื่อนที่ให้กับ Madogear ต้องมีไอเทม <Accelerator> ในการใช้สกิล และต้องการไอเทม Fuel Canister 1 ea ^000000", "[Level 1] : ^777777 ระยะเวลา 60 วินาที ^000000", "[Level 2] : ^777777 ระยะเวลา 90 วินาที ^000000", "[Level 3] : ^777777 ระยะเวลา 120 วินาที ^000000"}, 
[SKID.NC_HOVERING] = {"Hover", "MAX Lv : 1", "^777777Requirement :  Acceleration 1 ^000000", "ประเภท : ^777777  Active / Buff ^000000", "รายละเอียด : ^777777  ลอยสูงขึ้นจากพื้นทำให้ไม่ได้รับผลจากสกิลกับดัก และสกิลที่มีผลลงบนพื้นทั้งหลาย ต้องมีใส่ไอเทม <Hovering Booster> ในการใช้สกิล และต้องการไอเทม Fuel Canister 1 ea^000000", "[Level 1] : ^777777 ระยะเวลา 90 วินาที ^000000"}, 
